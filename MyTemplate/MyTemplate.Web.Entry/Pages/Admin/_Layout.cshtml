<!DOCTYPE html>
<html lang="zh">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <link rel="stylesheet" href="~/libs/bootstrap/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/libs/font-awesome/css/font-awesome.min.css"/>
    <link rel="stylesheet" href="~/libs/admin-lte/css/AdminLTE.min.css"/>
    <link rel="stylesheet" href="~/libs/admin-lte/css/skins/_all-skins.css"/>
    <link rel="stylesheet" href="~/libs/bootstrap-datepicker/css/bootstrap-datepicker.min.css"/>
    @await RenderSectionAsync("css", required: false)
    <title>@ViewData["Title"] - DashBoard</title>
</head>
<body class="skin-blue sidebar-mini" style="height: auto; min-height: 100%;">
<div class="wrapper" style="height: auto; min-height: 100%;">


    @await Html.PartialAsync("_Header")
    @await Html.PartialAsync("_Sidebar")
    <div class="content-wrapper" style="height: 971px">
        <section class="content-header">
            <h1>
                Dashboard
                <small>Control panel</small>
            </h1>
            <ol class="breadcrumb">
                <li>
                    <a href="#"><i class="fa fa-dashboard"></i> Home</a>
                </li>
                <li class="active">Dashboard</li>
            </ol>
        </section>
        <section class="content">
            @RenderBody()
        </section>

    </div>

    <script src="~/libs/jquery/jquery.min.js"></script>
    <script src="~/libs/bootstrap/js/bootstrap.js"></script>
    <script src="~/libs/admin-lte/js/adminlte.js"></script>
    <script src="~/libs/fastclick/fastclick.min.js"></script>
    <script src="~/libs/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/libs/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>
    <script src="~/libs/jquery-validate/jquery.validate.min.js"></script>
    <script src="~/libs/jquery-validate-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</div>
</body>
</html>